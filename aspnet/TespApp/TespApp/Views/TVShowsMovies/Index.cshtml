@model List<TespApp.Models.TVShowsMoviesIndexViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h3>Lista de peliculas</h3>
<hr />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Año de salida</th>
            <th>Nombre</th>
            <th>Genero</th>
            <th>Tipo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.AnnoSalida</th>
                <th>@item.Nombre.</th>
                <th>@item.genero.</th>
                <th>@item.Tipo</th>
            </tr>
        }
    </tbody>
</table>